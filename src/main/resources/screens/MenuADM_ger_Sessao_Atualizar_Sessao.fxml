<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="screenscontrollers.MenuAdmAtualizarSessaoController">
    <children>
        <MenuBar VBox.vgrow="NEVER" />
        <AnchorPane prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#fffade"
                           height="413.0" layoutX="-17.0" layoutY="-6.0"
                           stroke="BLACK" width="707.0" />
                <Label layoutX="312.0" text="Menu ADM" textFill="#9a89b4">
                    <font>
                        <Font name="System Bold" size="36.0" />
                    </font>
                </Label>
                <Button fx:id="botaoVoltar" layoutX="14.0" layoutY="14.0"
                        text="Voltar" textFill="#2c1477" onAction="#voltarParaMenuADM" />
                <Line endX="648.0" layoutX="38.0" layoutY="58.0"
                      startX="-100.0" />
                <Line endX="76.5" endY="325.0" layoutX="97.0"
                      layoutY="72.0" startX="76.5" startY="-68.0" />
                <Button fx:id="botaoAdicionarSessao" layoutX="29.0"
                        layoutY="87.0" prefHeight="25.0" prefWidth="130.0"
                        text="Adicionar Sessao" textFill="#2c1477"
                        onAction="#irParaTelaAdicionarSessao" />
                <Button fx:id="botaoListarSessoes" layoutX="29.0"
                        layoutY="131.0" prefHeight="25.0" prefWidth="130.0"
                        text="Listar Sessoes" textFill="#2c1477"
                        onAction="#irParaTelaListarSessoes" />
                <Button fx:id="botaoAtualizarSessao" layoutX="29.0"
                        layoutY="168.0" prefHeight="25.0" prefWidth="130.0"
                        text="Atualizar sessão" textFill="#2c1477" />
                <Button fx:id="botaoDeletarSessao" layoutX="29.0"
                        layoutY="210.0" prefHeight="25.0" prefWidth="130.0"
                        text="Deletar Sessão" textFill="#2c1477"
                        onAction="#irParaTelaDeletarSessao" />
                <Text fill="#537eda" layoutX="239.0" layoutY="94.0"
                      text="Atualizar Sessão" wrappingWidth="353.884765625">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Text>
                <Text fill="#537eda" layoutX="239.0" layoutY="126.0"
                      text="Preencha as Informações atualizadas abaixo"
                      wrappingWidth="353.884765625">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <Text fill="#537eda" layoutX="216.0" layoutY="153.0"
                      text="ID da sessão que deseja atualizar:">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <TextField fx:id="idSessaoField" layoutX="326.0" layoutY="144.0" />
                <Text fill="#537eda" layoutX="216.0" layoutY="198.0" text="Nome:">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <TextField fx:id="nomeSessaoField" layoutX="326.0" layoutY="181.0" />
                <Text fill="#537eda" layoutX="216.0" layoutY="240.0" text="Local:">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <TextField fx:id="localSessaoField" layoutX="326.0" layoutY="223.0" />
                <Text fill="#537eda" layoutX="216.0" layoutY="275.0" text="Horário:">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <TextField fx:id="horarioSessaoField" layoutX="326.0" layoutY="258.0" />
                <Text fill="#537eda" layoutX="216.0" layoutY="307.0" text="Evento Relacionado:">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <TextField fx:id="eventoRelacionadoField" layoutX="326.0" layoutY="298.0" />
                <Text fill="#537eda" layoutX="216.0" layoutY="354.0" text="SubEvento Relacionado:">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Text>
                <TextField fx:id="subEventoRelacionadoField" layoutX="326.0" layoutY="345.0" />
                <Button fx:id="botaoSalvarAlteracoes" layoutX="480.0" layoutY="380.0"
                        text="Salvar Alterações" onAction="#salvarAlteracoes" />
            </children>
        </AnchorPane>
    </children>
</VBox>
